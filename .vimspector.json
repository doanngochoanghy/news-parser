{
  "configurations": {
    "celery": {
      "adapter": "debugpy",
      "variables": {
        "VirtualEnv": {
          "shell": ["pipenv", "--venv"]
        }
      },
      "configuration": {
        "name": "<Python>: Launch",
        "type": "python",
        "request": "launch",
        "cwd": "${workspaceRoot}",
        "python": "${VirtualEnv}/bin/python",
        "stopOnEntry": false,
        "console": "externalTerminal",
        "debugOptions": [],
        "module": "celery",
        "args": ["-A", "fb_app", "worker", "--loglevel=info"]
      },
      "breakpoints": {
        "exception": {
          "raised": "N",
